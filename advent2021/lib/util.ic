-- ::= import "core.ic"
cstd ::= import "c/stdlib.ic"

#{export}
ParseInt64 ::= (s: []char) -> i64 {
  return cstd.atol(s.data)
}
#{export}
ParseInt64 ::= (c: char) -> i64 {
  return (c as i64) - (!0 as i64)
}

#{export}
Abs ::= (x: i64) -> i64 {
  if (x >= 0) then {
    return x
  } else {
    return -x
  }
}